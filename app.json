      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "62f95fa0-74fe-4906-9b48-13b5d70e840c"
      }
    },
    "owner": "impra",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/62f95fa0-74fe-4906-9b48-13b5d70e840c"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Meals to Go to access your camera."
        }
      ]
    ]
  }
}
